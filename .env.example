DATABASE_URL=postgresql+psycopg://postgres:postgres@postgres:5432/iats
REDIS_URL=redis://redis:6379/0

# LLM selection: local (Ollama) or openai
LLM_PROVIDER=local

# OpenAI settings (used when LLM_PROVIDER=openai)
OPENAI_API_KEY=
OPENAI_MODEL=gpt-5.3-codex

# Local LLM settings (used when LLM_PROVIDER=local)
LOCAL_LLM_MODEL=qwen2.5:7b-instruct
OLLAMA_BASE_URL=http://ollama:11434
LOCAL_LLM_TIMEOUT_SECONDS=300

AWS_REGION=us-east-1
FIXTURE_MODE=true
SLACK_WEBHOOK_URL=
REPO_BASE_PATH=/workspace/repos
ALLOW_RAW_STORAGE=false
TRIAGE_WINDOW_MINUTES=10
MAX_REPO_SNIPPETS=5
